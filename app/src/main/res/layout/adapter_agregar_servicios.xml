<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


    <ImageView
            android:id="@+id/imageAdapterSearchServices"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_marginStart="32dp"
            android:layout_marginTop="16dp"
            android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/aviario" />

    <TextView
            android:id="@+id/descripcionAdapterSearchService"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="32dp"
            android:fontFamily="@font/coda"
            android:text="@string/nombre_servicios"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageAdapterSearchServices"
            app:layout_constraintTop_toTopOf="parent" />

    <TextView
            android:id="@+id/organizerAdapterSearchInter"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="100dp"
            android:fontFamily="@font/coda"
            android:text="@string/organizado_por"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageAdapterSearchServices"
            app:layout_constraintTop_toBottomOf="@+id/descripcionAdapterSearchService" />

    <TextView
            android:id="@+id/organizerTextAdapterSearchInter"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="7dp"
            android:layout_marginEnd="80dp"
            android:fontFamily="@font/coda"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageAdapterSearchServices"
            app:layout_constraintTop_toBottomOf="@+id/organizerAdapterSearchInter" />
</androidx.constraintlayout.widget.ConstraintLayout>